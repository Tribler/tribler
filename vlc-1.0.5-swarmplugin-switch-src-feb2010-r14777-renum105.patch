+///////////////////////////
+void P2PControl::unregEventCB( bg_event_t event )
+{
+  if( WaitForSingleObject( mSyncMutex, INFINITE ) == WAIT_FAILED )
+  { writeOnLog( "Failed to Lock the Mutex" ); return; }
+
+  mEventCBMap.erase( event );
+
