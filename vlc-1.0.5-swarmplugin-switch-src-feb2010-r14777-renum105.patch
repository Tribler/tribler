+static PyObject *
+vlcMedia_get_meta( PyObject *self, PyObject *args )
+{
+    libvlc_exception_t ex;
+    char * psz_meta = NULL;
+    char * psz_ret = NULL;
+    PyObject* o_ret;
+    int i_index = -1;
+    int i_loop = 0;
